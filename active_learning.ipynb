   "source": [
    "# load the svm model\n",
    "with open('models/feas_model.pkl', 'rb') as f:    \n",
    "    feas_model = pickle.load(f)\n",
    "\n",
    "lrr_cutoff = 1/np.log10(150+10)\n",
    "feas_func = lambda x: feas_model.predict(x)[0] > lrr_cutoff\n",
    "\n",
    "# Generate composition for the next round\n",
    "g = UniformCompositionGenerator(materials=X_COLS, n_comps=22, n_iters=10000, random_state=1,\n",
    "                                existing_comps=df_train[X_COLS],\n",
    "                                perf_func=perf_models_eps10.predict_variance,\n",
    "                                feas_func=feas_func)\n",
    "\n",
    "comps = g.optimize()\n",
    "comps = pd.DataFrame(comps, columns=X_COLS)\n",
    "comps"
   ]
