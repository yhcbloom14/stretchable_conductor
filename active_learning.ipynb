    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib as mpl\n",
    "import matplotlib.pyplot as plt\n",
    "import copy\n",
    "import pickle\n"
    "import tensorflow as tf\n",
    "import keras\n",
    "from sklearn.utils import Bunch\n",
    "from scikeras.wrappers import KerasRegressor\n",
    "from sklearn.model_selection import cross_validate, KFold\n",
    "\n",
    "from utils.ml import MLPNetwork, MOVotingRegressor, build_lr_mlp_model\n",
    "from utils.al import UniformCompositionGenerator\n",
    "\n",
    "import warnings\n",
    "warnings.filterwarnings(\"ignore\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "507c0055",
   "metadata": {},
   "outputs": [],
   "source": [
    "#  Load active learning data\n",
    "round_num = 1   # active learning round\n",
    "df_train = pd.read_csv('data/al_train_rd01_DA.csv')\n",
    "\n",
    "X_COLS = ['CNT', 'MXN', 'GNP', 'PVA', '1D', '2D','2D1D','2D2D','Pre-stretch', 'thickness']\n",
    "Y_COL = 'eps10'\n",
    "\n",
    "x_train = df_train[X_COLS]\n",
    "y_train = df_train[Y_COL]"
   ]
  },
  {
   "cell_type": "code",
