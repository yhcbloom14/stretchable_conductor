   "source": [
    "params = {'n_max_nodes': 8,\n",
    "  'n_layers': 3,\n",
    "  'lr': 2e-05,\n",
    "  'batch_size': 512,\n",
    "  'epochs': 8192,\n",
    "  'random_seed': 0}\n",
    "\n",
    "#  Train the ANN\n",
    "tf.keras.backend.clear_session()\n",
    "tf.keras.utils.set_random_seed(int(params['random_seed']))\n",
    "\n",
    "perf_model_eps10 = KerasRegressor(\n",
    "    model=build_lr_mlp_model,\n",
    "    output_scale=10, verbose=0, **params)\n",
    "\n",
    "perf_model_eps10.fit(x_train, y_train)\n",
    "\n",
    "k_fold = 5\n",
    "cv_results = cross_validate(perf_model_eps10, x_train, y_train,\n",
    "                            scoring=['neg_mean_absolute_percentage_error'],\n",
    "                            cv=KFold(n_splits=k_fold, shuffle=True, random_state=0),\n",
    "                            return_train_score=True, return_estimator=True)\n",
    "\n",
    "cv_models = cv_results['estimator']\n",
    "perf_models_eps10 = MOVotingRegressor([(str(i), MLPNetwork.from_keras(m.model_)) for i, m in enumerate(cv_models)])\n",
    "names, clfs = perf_models_eps10._validate_estimators()\n",
    "perf_models_eps10.estimators_ = [clf for name, clf in zip(names, clfs) if name != 'drop']\n",
    "perf_models_eps10.named_estimators_ = Bunch(**{name: clf for name, clf in zip(names, clfs) if name != 'drop'})"
   ]
  },
